{
  "name": "rdm-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "cross-env NODE_ENV=production  vite build --mode production && esno ./build/script/postBuild.ts",
