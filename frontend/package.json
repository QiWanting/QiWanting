    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-define-config": "^2.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-icons": "^0.18.1",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vite-plugin-mock": "2.9.6",
    "vue-tsc": "^1.8.19"
